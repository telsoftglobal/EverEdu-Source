$('#educations_list').prepend('<%= escape_javascript(render(:partial  => 'educations/education', locals: {education: @education})) %>');

$('#educationModal').modal('hide');

var editElement = $('#education<%= @education.id %>').find(".editEducation");
$(editElement).bind('click', function (e) {
    openEditEducationModal(this, e);
});

var deleteElement = $('#education<%= @education.id %>').find(".deleteEducation");
$(deleteElement).bind('click', function (e) {
    processDeleteEducation(this, e);
});